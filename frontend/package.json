{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@formatjs/intl": "^1.7.1",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.1",
    "babel-plugin-react-intl": "^8.2.25",
    "intl": "^1.2.5",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-froala-wysiwyg": "^3.2.6-1",
    "react-html-parser": "^2.0.2",
    "react-intl": "^5.12.5",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "react-token-auth": "^1.1.8",
    "web-vitals": "^1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "rm -rf ../backend/templates && rm -rf ../backend/static/css && rm -rf ../backend/static/js && rm -rf ../backend/static/media",
    "build": "react-scripts build",
    "postbuild": "cp -r ./build/ ../backend/templates && mv ../backend/templates/static/* ../backend/static/",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "extract-messages": "set NODE_ENV=production&& babel ./src >NUL"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-react-jsx"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}